<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pawdaw.travel_calendar">

    <uses-permission android:name="android.permission.INTERNET" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>
    <meta-data
        android:name="android.app.default_searchable"
        android:value=".List_Fragment" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
    <activity
        android:name="com.example.pawdaw.travel_calendar.EditableActivity"
        android:label="Edit your note"
        android:parentActivityName=".MainActivity">
    </activity>
    <activity
        android:name="com.example.pawdaw.travel_calendar.NewNoteActivity"
        android:label="Create new note"
        android:parentActivityName=".MainActivity">
    </activity>

        
    </application>

</manifest>
